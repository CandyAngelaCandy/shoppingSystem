.site-nav-bd
  background: #f5f5f5
  height:50px
  line-height:50px
  .site-nav-bd::after
    content: ""
    display: table
    clear: both
  .site-nav-bd-list
     width:70%
     margin: 0 auto
  .site-nav-bd-list li
    text-align:center
  .site-nav-system-title
    padding-left:100px
    font-size:27px
    float:left
  .site-nav-my-cart
    float:right
    font-size:15px
  .site-nav-my-cart a
    cursor:pointer

